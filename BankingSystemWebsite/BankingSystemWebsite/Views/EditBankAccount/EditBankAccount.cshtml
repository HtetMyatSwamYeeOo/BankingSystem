@using BankingSystemWebsite.Models.BankAccount

@{
    ViewBag.Title = "EditBankAccount";
}

<!DOCTYPE html>
<html lang="en">
<head>

    <script src='@Url.Content("~/Scripts/bootstrap.js")' type="text/javascript"></script>

    @{Html.RenderPartial("_importLayout"); }






</head>


<body>

    <!-- container section start -->
    <section id="container" class="">
        <!--Navigation-->
        @{Html.RenderPartial("_navLayout"); }

        <section id="main-content">
            <section class="wrapper">
                <div class="row">
                    <div class="col-lg-12">
                        <h3 class="page-header"><img src="~/Image/bank.png" style="height:40px; margin: 0 0 20px;" />&nbsp;&nbsp; EditBankAccount</h3>




                        @{
                            string[] temp = { "", "" };
                            temp[0] = "updateBankAccount";
                            temp[1] = "EditBankAccount";

                            TempData["temp_array"] = temp;

                        }


                        @{
                            NewBankAccountObject n = new NewBankAccountObject();
                            n = (NewBankAccountObject)TempData["EditID"];
                            TempData["EditID"] = n;

                        }

                        @{Html.RenderAction("editNewBankAccount", "_newBankAccountLayout"); }
                    </div>
                </div>

            </section>

            <!--main content end-->

        </section>
    </section>
    <script>

                //knob
                $(function () {
                    $(".knob").knob({
                        'draw': function () {
                            $(this.i).val(this.cv + '%')
                        }
                    })
                });

                //carousel
                $(document).ready(function () {
                    $("#owl-slider").owlCarousel({
                        navigation: true,
                        slideSpeed: 300,
                        paginationSpeed: 400,
                        singleItem: true

                    });
                });

                //custom select box

                $(function () {
                    $('select.styled').customSelect();
                });

                /* ---------- Map ---------- */
                $(function () {
                    $('#map').vectorMap({
                        map: 'world_mill_en',
                        series: {
                            regions: [{
                                values: gdpData,
                                scale: ['#000', '#000'],
                                normalizeFunction: 'polynomial'
                            }]
                        },
                        backgroundColor: '#eef3f7',
                        onLabelShow: function (e, el, code) {
                            el.html(el.html() + ' (GDP - ' + gdpData[code] + ')');
                        }
                    });
                });

    </script>

</body>
</html>

